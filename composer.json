{
    "name": "drupal/parse-composer",
    "authors": [
        {
            "name": "Will Milton",
            "email": "wa.milton@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/drupal",
                "version": "7.43.0",
                "dist":
                {
                    "url": "https://ftp.drupal.org/files/projects/drupal-7.43.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Drupal\\ParseComposer\\": "src/"
        },
        "files": [
            "bootstrap.php"
        ]
    },
    "require": {
        "php": ">=5.6",
        "drupal/drupal": "7.*",
        "guzzle/guzzle": "*",
        "composer/semver": "^1.4",
        "igorw/get-in": "^1.0",
        "composer/composer": "^1.0",
        "leaphub/phpcs-symfony2-standard": "^2.0"
    },
    "require-dev": {
        "phpspec/phpspec": "2.*",
        "phpunit/phpunit": "4.4.*",
        "symfony/symfony": "^2.8",
        "squizlabs/php_codesniffer": "2.*"
    }
}
